{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>tksst/renovate-config"],
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchPackageNames": ["constructs", "aws-cdk-lib"],
      "enabled": false
    },
    {
      "groupName": "esbuild",
      "matchManagers": ["npm"],
      "matchPackageNames": ["esbuild"]
    },
    {
      "groupName": "@aws-sdk/client-ec2",
      "matchManagers": ["npm"],
      "matchPackageNames": ["@aws-sdk/client-ec2"]
    }
  ]
}
