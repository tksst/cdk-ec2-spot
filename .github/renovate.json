{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>tksst/renovate-config"],
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchPackageNames": ["constructs", "aws-cdk-lib"],
      "enabled": false
    },
    {
      "groupName": "esbuild",
      "matchManagers": ["npm"],
      "matchPackageNames": ["esbuild"]
    }
  ],
  "baseBranches": ["develop"]
}
