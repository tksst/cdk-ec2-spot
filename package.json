{
  "name": "cdk-ec2-spot-simple",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "homepage": "https://github.com/tksst/cdk-ec2-spot-simple/",
  "repository": "github:tksst/cdk-ec2-spot-simple",
  "license": "Apache-2.0",
  "author": {
    "name": "Takashi Sato",
    "email": "takashi@tks.st",
    "url": "https://www.tks.st/"
  },
  "type": "module",
  "exports": {
    "import": "./dist/lib/index.js",
    "require": "./dist/lib/index.cjs"
  },
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "clean": "tss-rmrf ./dist/",
    "build": "run-s --continue-on-error clean build:*",
    "build:lib": "tsup --config ./tsup.config.lib.js",
    "build:lambda": "tsup --config ./tsup.config.lambda.js",
    "prepack": "pnpm build",
    "watch": "npm-run-all clean --parallel --continue-on-error --print-label watch:*",
    "watch:lib": "pnpm build:lib --watch",
    "watch:lambda": "pnpm build:lambda --watch",
    "fix": "run-s --continue-on-error fix:*",
    "fix:prettier": "pnpm lint:prettier --write",
    "fix:eslint": "pnpm lint:eslint --fix",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:prettier": "prettier --cache --check .",
    "lint:eslint": "eslint --color --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/ --ext .js,.cjs,.mjs,.ts,.cts,.mts .",
    "lint:secretlint": "secretlint --maskSecrets **",
    "lint:tsc": "tsc --noEmit",
    "test": "run-s test:*",
    "test:unit": "jest --ci"
  },
  "devDependencies": {
    "@aws-sdk/client-ec2": "3.241.0",
    "@secretlint/secretlint-rule-preset-recommend": "6.0.1",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/git": "10.0.1",
    "@swc/core": "1.3.24",
    "@swc/jest": "0.2.24",
    "@tksst/eslint-config": "4.2.0",
    "@tksst/prettier-config": "1.1.0",
    "@tksst/typescript-starter-configs": "2.0.0",
    "@types/aws-lambda": "8.10.109",
    "@types/jest": "29.2.5",
    "@types/node": "14.18.36",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "aws-cdk-lib": "2.24.0",
    "better-typescript-lib": "2.2.0",
    "constructs": "10.0.9",
    "eslint": "8.31.0",
    "eslint-plugin-jest": "27.2.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "29.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "secretlint": "6.0.1",
    "semantic-release": "19.0.5",
    "source-map-support": "0.5.21",
    "tsup": "6.5.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.24.0",
    "constructs": "^10.0.9"
  },
  "packageManager": "pnpm@7.21.0",
  "engines": {
    "node": ">= 14"
  }
}
